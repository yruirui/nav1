const hashmap=[
    {logo:'A',url:''}
]

$('.add').on('click',()=>{
   let url= window.prompt('请输入要添加的网址')
   if(url.indexOf('http')!==0){
        url='https://'+url
   }
   let $last=$('.last')
   const $site=$(`<li><a href="${url}">
   <div class="site">
       <div class="logo">${url[8]}</div>
       <div class="link">${url}</div>
   </div>
   </a>
  </li>`).insertBefore($last)
})